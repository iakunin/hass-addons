{
  "name": "Amazon S3 Backup",
  "version": "1.3.1",
  "slug": "amazon-s3-backup",
  "description": "Sync Backups to your Amazon S3 bucket",
  "url": "https://github.com/iakunin/hass-addons/blob/main/amazon-s3-backup",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "manual",
  "init": false,
  "startup": "once",
  "advanced": true,
  "hassio_api": true,
  "hassio_role": "backup",
  "options": {
    "monitor_path": "",
    "aws_access_key": "",
    "aws_secret_access_key": "",
    "bucket_name": "",
    "bucket_region": "",
    "endpoint_url": ""
  },
  "schema": {
    "monitor_path": "str",
    "aws_access_key": "str",
    "aws_secret_access_key": "password",
    "bucket_name": "str",
    "bucket_region": "str",
    "endpoint_url": "str"
  },
  "map": [
    "backup:rw"
  ]
}
